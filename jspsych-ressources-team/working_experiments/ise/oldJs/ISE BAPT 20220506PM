 var displayCross = {
      
      type: jsPsychHtmlKeyboardResponse,
      stimulus: function(){
        return `<p style="font-size: 70px"><b>+</b></p>`
      },
      on_start: function(){
        console.log(`Playing sound ${j} : ${shuffledSounds[j]}`)
         soundPlayed = shuffledSounds[j]
         audio = new Audio(shuffledSounds[j]);
         audio.loop = false;
         audio.play()
         console.log(soundPlayed)
         return soundPlayed
      },
      choices: "NO_KEYS",
      trial_duration: 2000,
      post_trial_gap: 0,
      
    };
    
 
